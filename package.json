{
  "name": "humm-widgets-cards",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "npm run start:au",
    "start:au": "LANG=au npm run start:lang",
    "start:nz": "LANG=nz npm run start:lang",
    "start:lang": "vue-cli-service serve ./src/main-${LANG}.ts",
    "build:lib": "vue-cli-service build --no-clean --target lib --inline-vue --name ${npm_package_name}-${LANG} ./src/main-${LANG}.ts",
    "build": "LANG=au npm run build:lib && LANG=nz npm run build:lib",
    "preview": "npm run build && npx serve -l 3000 --config ./../serve.json ./build",
    "lint": "vue-cli-service lint",
    "perf:svg": "npx svgo src/assets/images/*.svg -o src/assets/images/*.svg",
    "perf:build": "npx source-map-explorer build/*.js --no-border-checks",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start:cypress": "npx cypress run",
    "open": "npx cypress open",
    "record": "npx cypress run --record --key 3d96ba1c-7211-4e98-bd5c-30dc82ed7d30"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "vue": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "@storybook/addon-a11y": "6.3.7",
    "@storybook/addon-actions": "6.3.7",
    "@storybook/addon-essentials": "6.3.7",
    "@storybook/addon-links": "6.3.7",
    "@storybook/vue3": "6.3.7",
    "@types/jest": "24.0.19",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-plugin-typescript": "4.5.0",
    "@vue/cli-plugin-unit-jest": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/compiler-sfc": "3.0.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "2.0.0-rc.12",
    "babel-loader": "8.2.2",
    "cypress": "8.4.1",
    "eslint": "6.7.2",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.0.0",
    "prettier": "2.2.1",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "typescript": "4.1.5",
    "vue-jest": "4.0.1",
    "vue-loader": "16.5.0"
  }
}
